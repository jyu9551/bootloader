{
    "version": "2.0.0",
    "runner": "terminal",
    "type": "shell",
    "echoCommand": true,
    "presentation": {
        "reveal": "always"
    },
    "tasks": [
        {
            "type": "shell",
            "label": "gcc.exe build active file",
            "command": "C:/MinGW/bin/gcc.exe",
            "args": [
                "-g",
                "${fileDirname}/KISA_SHA256.c",
                "${file}",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe",
            ],
            "options": {
                "cwd": "C:/MinGW/bin"
            },
            "problemMatcher": [
                "$gcc"
            ]
        }
    ]
}